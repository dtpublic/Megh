<config>
	<!-- Following patterns define exclusions from obfuscation. Please add to 
		this list only when absolutely required. Typically this is the case only 
		when class or property names need to be exposed to the user (main classes 
		or bean properties). Ensure any such change remains limited to the minimum 
		required! For example, it may be necessary to preserve property names. But 
		that does not imply that class/package names also need to be kept! -->
	<keep-names>
		<class template="public class com.datatorrent.apps.ingestion.Application">
			<method access="public" />
			<field access="public" />
		</class>
		<field template="private+ java.lang.String *" />
	</keep-names>
	<property name="classes-naming" value="keep-$-sign" />
	<property name="synthetize-fields" value="disable" />
	<property name="synthetize-methods" value="disable" />
	<class template="public class com.datatorrent.apps.ingestion.io.output.OutputFileMetaData">
      <field access="public"/>
      <method access="public"/>
   </class>
   <class template="public class com.datatorrent.apps.ingestion.process.compaction.MetaFileWriter">
      <field access="public"/>
      <method access="public"/>
   </class>
   <class template="public class com.datatorrent.apps.ingestion.io.output.EncryptionMetaData">
      <field access="public"/>
      <method access="public"/>
   </class>
   <class template="public class com.datatorrent.apps.ingestion.lib.AsymmetricKeyManager">
      <field access="public"/>
      <method access="public"/>
   </class>
   <class template="public class com.datatorrent.apps.ingestion.lib.CipherProvider">
      <field access="public"/>
      <method access="public"/>
   </class>
   <class template="public class com.datatorrent.apps.ingestion.lib.SymmetricKeyManager">
      <field access="public"/>
      <method access="public"/>
   </class>
	<jars>
		<jar in="${obf.jar}" out="${obf.jar}" />
	</jars>
	<property name="log-file" value="target/allatori-log.xml" />
</config>

